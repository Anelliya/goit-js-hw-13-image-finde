{{#each this}}
<li class="photo-card">
    <div>
        <div class="image-container">
            <a href="{{this.largeImageURL}}">
                <img src={{this.previewURL}} alt={{this.tags}} class="image" />
            </a>
        </div>

        <div class="stats">
            <p class="stats-item">
                <i class="material-icons">thumb_up</i>
                {{this.likes}} </p>
            <p class="stats-item">
                <i class="material-icons">visibility</i>
                {{this.views}}
            </p>
            <p class="stats-item">
                <i class="material-icons">comment</i>
                {{this.comments}} </p>
            <p class="stats-item">
                <i class="material-icons">cloud_download</i>
                {{this.downloads}}
            </p>
            <a href={{this.pageURL}} target="_blank">
                <p>more...</p>
            </a>

        </div>
    </div>
</li>

{{/each}} {{!--  --}}